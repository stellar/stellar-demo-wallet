{
  "extends": "@stellar/tsconfig",
  "compilerOptions": {
    "baseUrl": "src/",
    "outDir": "build/",
    "target": "esnext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "esnext",
    "moduleResolution": "bundler",
    "noEmit": true,
    "jsx": "react-jsx",
    "allowSyntheticDefaultImports": true
  },
  "include": ["src"]
}
